.DS_Store

# jekyll

.jekyll-metadata
_site/

# ignore all extra bower_components
# only keep dependency source (not entire git repo)

# bootstrap
# only keep min css, min js, fonts

bower_components/bootstrap/*
!bower_components/bootstrap/dist/

bower_components/bootstrap/dist/*
!bower_components/bootstrap/dist/css
!bower_components/bootstrap/dist/fonts
!bower_components/bootstrap/dist/js

bower_components/bootstrap/dist/css/*
!bower_components/bootstrap/dist/css/*.min.css

bower_components/bootstrap/dist/js/*
!bower_components/bootstrap/dist/js/*.min.js

# font awesome

bower_components/font-awesome/*
!bower_components/font-awesome/css/
!bower_components/font-awesome/fonts

bower_components/font-awesome/css/*
!bower_components/font-awesome/css/*.min.css

# html5shiv

bower_components/html5shiv/*
!bower_components/html5shiv/dist

bower_components/html5shiv/dist/*
!bower_components/html5shiv/dist/*.min.js

# jquery

bower_components/jquery/*
!bower_components/jquery/dist

bower_components/jquery/dist/*
!bower_components/jquery/dist/*.min.js

# respond

bower_components/respond/*
!bower_components/respond/dest

bower_components/respond/dest/*
!bower_components/respond/dest/*.min.js
